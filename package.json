{
  "name": "nextjs-starter",
  "description": "A Next.js starter that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "clare chang<cwuyiqing@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "export": "next export",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "esbuild": "0.13.3",
    "gray-matter": "4.0.3",
    "mdx-bundler": "6.0.2",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/forms": "0.3.3",
    "@types/node": "15.12.2",
    "@types/prettier": "2.3.2",
    "@types/react": "17.0.9",
    "@types/react-dom": "17.0.6",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "autoprefixer": "10.2.6",
    "eslint": "7.28.0",
    "eslint-config-alloy": "4.1.0",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "globby": "12.0.0",
    "husky": "7.0.1",
    "lint-staged": "11.0.0",
    "next-sitemap": "1.6.140",
    "postcss": "8.3.0",
    "prettier": "2.3.1",
    "tailwind-scrollbar": "1.3.1",
    "tailwindcss": "2.1.4",
    "typescript": "4.3.2"
  }
}
